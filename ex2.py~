#!/usr/bin/python
def add(x,y):  #function that adds two variables
    print "add %d + %d" %(x,y)
    return x + y

def multiply(x, y): #function that multiplies two variables
    print "multiply %d * %d" %(x,y)
    return x * y
    
def divide(x, y): #function that divides one variable with the other
    print "divide %d / %d" %(x, y)
    return x / y
    
